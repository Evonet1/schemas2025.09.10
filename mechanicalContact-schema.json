  "$comment": "This schema is for any mechanical contact - isolator, breaker, connector",
  "schema": {
    "type": "object",
    "properties": {
      "maxVoltageAC": { "$ref": "voltageRating-schema.json#/schema" },
      "maxVoltageDC": { "$ref": "voltageRating-schema.json#/schema" },
      "maxSteadyCurrent": { "$ref": "currentRating-schema.json#/schema" },
      "ampsBreakingCapacity": { "$ref": "currentRating-schema.json#/schema" },
      "minimumWhettingCurrent": { "$ref": "currentRating-schema.json#/schema" },
      "prechargeProtectionRequired": { "type": "boolean" },
      "configuration": {
        "type": "string",
        "enum": [ "Normally Open", "Normally Closed", "Change-over", "Make-before-break" ]
      }
    }
  }
}
