  "$comment": "This schema may be used for any resistance.  Note unicode points may need different syntax, depending on usage",
  "schema": {
    "oneOf": [
      {
        "type": "object",
        "properties": {
          "value": {"type": "number"},
          "units": {
            "type": "string",
            "enum": [
              "milliohms",
              "ohms",
              "kiloohms. megohms",
              "m\u03A9",
              "\u03A9",
              "k\u03A9",
              "M\u03A9",
              "mΩ",
              "Ω",
              "kΩ",
              "MΩ"
            ]
          }
        },
        "required": [
          "value",
          "units"
        ]
      },
      {
        "type": "string",
        "pattern": "^([0-9]+(.[0-9]+)?)(m|k|M)?(ohms|Ω|\u03A9)$"
      }
    ]
  }
}